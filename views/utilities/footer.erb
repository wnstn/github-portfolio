<script>
  var loadTime = 0;

  if (loadTimeStart) {
    window.addEventListener("load", function (e) {
      loadTime = e.timeStamp - loadTimeStart;
      logEvent();
    });
  } else {
    logEvent();
  }

  function logEvent() {
    var visit = {
      page: "Winston.cc",
      referrer: document.referrer,
      width: window.outerWidth,
      loadTime: loadTime,
      keen: {
        timestamp: new Date().toISOString()
      }
    }

    Keen.addEvent('visit', visit);
  }
</script>
